{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controller/**/*.ts"],
  "spec": {
    "basePath": "/api",
    "outputDirectory": "src",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/api",
    "routesDir": "src"
  },
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"]
  }
}